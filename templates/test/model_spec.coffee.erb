# Spec for <%= camelize_name %>Model

define [
  'chaplin'
  'models/<%= underscore_name %>'
], (Chaplin, <%= camelize_name %>Model) ->
  'use strict'

  describe 'Models/<%= camelize_name %>Model', ->

    beforeEach ->
      @<%= camelize_name %>Model = new <%= camelize_name %>Model()

    describe '@initialize', ->

      it 'should have a initialize function', ->
        expect(@<%= camelize_name %>Model.initialize).to.be.a('function')

      it 'should be called on startup'
        spy = sinon.spy(@<%= camelize_name %>Model.__proto__, 'initialize')
        @<%= camelize_name %>Model.initialize()
        expect(spy.calledOnce).to.be.true